<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="md2viewer" />
		<Option pch_mode="2" />
		<Option compiler="armelfgcc" />
		<Build>
			<Target title="release">
				<Option output="md2viewer" prefix_auto="0" extension_auto="0" />
				<Option object_output="..\objs\release" />
				<Option type="1" />
				<Option compiler="armelfgcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-msoft-float" />
					<Add option="-fomit-frame-pointer" />
					<Add option="-O2" />
					<Add option="-include AndroidConfig.h" />
					<Add option="-fpic" />
					<Add option="-ffunction-sections" />
					<Add option="-funwind-tables" />
					<Add option="-fstack-protector" />
					<Add option="-fno-short-enums" />
					<Add option="-fno-rtti" />
					<Add option="-fstrict-aliasing" />
					<Add option="-fno-exceptions" />
					<Add option="-DANDROID" />
					<Add directory="..\include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-nostdlib -Bdynamic" />
					<Add option="-Wl,-T,armelf.x" />
					<Add option="-Wl,-dynamic-linker,/system/bin/linker" />
					<Add option="E:\toolchain\arm-eabi\lib\crtbegin_dynamic.o" />
					<Add option="E:\toolchain\arm-eabi\lib\crtend.o" />
					<Add library="..\lib\libf3d.a" />
					<Add library="c" />
					<Add library="m" />
					<Add library="stdc++" />
					<Add library="GLES_CM" />
				</Linker>
				<ExtraCommands>
					<Add after="adb push md2viewer /dev/sample/md2viewer" />
					<Add after="adb shell chmod 777 /dev/sample/md2viewer" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="debug">
				<Option output="md2viewer_d" prefix_auto="0" extension_auto="0" />
				<Option object_output="..\objs\debug" />
				<Option type="1" />
				<Option compiler="armelfgcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-g" />
					<Add option="-include AndroidConfig.h" />
					<Add option="-fpic" />
					<Add option="-ffunction-sections" />
					<Add option="-funwind-tables" />
					<Add option="-fstack-protector" />
					<Add option="-fno-short-enums" />
					<Add option="-fno-rtti" />
					<Add option="-fstrict-aliasing" />
					<Add option="-fno-exceptions" />
					<Add option="-fomit-frame-pointer" />
					<Add option="-DANDROID" />
					<Add option="-DDEBUG" />
					<Add directory="..\include" />
				</Compiler>
				<Linker>
					<Add option="-nostdlib -Bdynamic" />
					<Add option="-Wl,-T,armelf.x" />
					<Add option="-Wl,-dynamic-linker,/system/bin/linker" />
					<Add option="E:\toolchain\arm-eabi\lib\crtbegin_dynamic.o" />
					<Add option="E:\toolchain\arm-eabi\lib\crtend.o" />
					<Add library="..\lib\libf3d_d.a" />
					<Add library="c" />
					<Add library="GLES_CM" />
				</Linker>
				<ExtraCommands>
					<Add after="adb push md2viewer_d /dev/sample/md2viewer_d" />
					<Add after="adb shell chmod 777 /dev/sample/md2viewer_d" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<Unit filename="..\test\md2viewer.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
