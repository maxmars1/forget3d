<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="lights" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="release">
				<Option output="..\..\data\lights" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\..\data" />
				<Option object_output="..\..\objs\win32Rel" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-DNDEBUG" />
					<Add option='-DUSE_WRAPPER_GL=\&quot;libGLES_CM_NoE.dll\&quot;' />
					<Add option='-DIS_ALONE_EGL=\&quot;libEGL.dll\&quot;' />
					<Add directory="..\..\include" />
					<Add directory="..\include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="kernel32" />
					<Add library="user32" />
					<Add library="gdi32" />
					<Add library="f3d" />
					<Add directory="..\..\lib\win32" />
				</Linker>
			</Target>
			<Target title="debug">
				<Option output="..\..\data\lights_d" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\..\data" />
				<Option object_output="..\..\objs\win32Dbg" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-DDEBUG" />
					<Add option='-DUSE_WRAPPER_GL=\&quot;libGLES_CM_NoE.dll\&quot;' />
					<Add option='-DIS_ALONE_EGL=\&quot;libEGL.dll\&quot;' />
					<Add directory="..\..\include" />
					<Add directory="..\include" />
				</Compiler>
				<Linker>
					<Add library="kernel32" />
					<Add library="user32" />
					<Add library="gdi32" />
					<Add library="f3d_d" />
					<Add directory="..\..\lib\win32" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="..\..\test\lights.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
